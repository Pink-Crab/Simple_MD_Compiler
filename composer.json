{
    "name": "pinkcrab/wp-scoper-patcher-provider",
    "type": "library",
    "description": "PHP Scoper patcher for all core WordPress symbols.",
    "keywords": [],
    "homepage": "https://pinkcrab.co.uk",
    "license": "MIT",
    "authors": [{
        "name": "Glynn Quelch",
        "email": "glynn.quelch@pinkcrab.co.uk",
        "homepage": "http://clappo.co.uk",
        "role": "Developer"
    }],
    "autoload": {
        "psr-4": {
            "PinkCrab\\Simple_MD_Compiler\\": "src"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "PinkCrab\\Simple_MD_Compiler\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.0",
        "symfony/var-dumper": "<=6.2.7",
        "phpstan/phpstan": "1.*",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "require": {
        "php": ">=7.4.0",
        "mnapoli/silly": "^1.7",
        "level-2/dice": "^4.0.2",
        "psr/http-message": "^1.0",
        "pinkcrab/memoize-trait": "^1.0"

    },
    "scripts": {
        "test": "phpunit --coverage-clover coverage.xml",
        "coverage": "phpunit --coverage-html coverage-report",
        "analyse": "vendor/bin/phpstan analyse src -l8",
        "sniff": "./vendor/bin/phpcs src/ -v",
        "all": "composer test && composer analyse && composer sniff"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}